<div class="jumbotron">
  <div class='container'>
    <h1>new post</h1>
    <%= form_with model: @post, url:'/posts', local: true do |f| %>
      <h4>タイトル</h4>
      <%= f.text_field :title %>
      <h4>内容</h4>
      <%= f.text_area :body %>
      <h4>画像</h4>
      <%= f.file_field :image %>
      <%= f.submit '投稿', class: 'btn btn-info' %>
    <% end %>
    <br>
    <%= link_to "TOP", top_path %>
  </div>
</div>